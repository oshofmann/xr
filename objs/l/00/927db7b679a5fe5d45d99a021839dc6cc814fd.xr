"", 
"<span class=\"ts\"/>if (<a class=\"id\" href=\"#rb\">rb</a>)", 
"<span class=\"ts\"/>    <a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#flags\">flags</a> |= <a class=\"id\" href=\"#FB_CVT_FLAG_REDUCED_BLANK\">FB_CVT_FLAG_REDUCED_BLANK</a>;", 
"", 
"<span class=\"ts\"/>if (<a class=\"id\" href=\"#mode\">mode</a>-><a class=\"id\" href=\"#vmode\">vmode</a> &amp; <a class=\"id\" href=\"#FB_VMODE_INTERLACED\">FB_VMODE_INTERLACED</a>)", 
"<span class=\"ts\"/>    <a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#flags\">flags</a> |= <a class=\"id\" href=\"#FB_CVT_FLAG_INTERLACED\">FB_CVT_FLAG_INTERLACED</a>;", 
"", 
"<span class=\"ts\"/><a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#xres\">xres</a> = <a class=\"id\" href=\"#mode\">mode</a>-><a class=\"id\" href=\"#xres\">xres</a>;", 
"<span class=\"ts\"/><a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#yres\">yres</a> = <a class=\"id\" href=\"#mode\">mode</a>-><a class=\"id\" href=\"#yres\">yres</a>;", 
"<span class=\"ts\"/><a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#refresh\">refresh</a> = <a class=\"id\" href=\"#mode\">mode</a>-><a class=\"id\" href=\"#refresh\">refresh</a>;", 
"<span class=\"ts\"/><a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#f_refresh\">f_refresh</a> = <a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#refresh\">refresh</a>;", 
"<span class=\"ts\"/><a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#interlace\">interlace</a> = 1;", 
"", 
"<span class=\"ts\"/>if (!<a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#xres\">xres</a> || !<a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#yres\">yres</a> || !<a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#refresh\">refresh</a>) {", 
"<span class=\"ts\"/><span class=\"ts\"/><a class=\"id\" href=\"#printk\">printk</a>(<a class=\"id\" href=\"#KERN_INFO\">KERN_INFO</a> \"fbcvt: Invalid input parameters\\n\");", 
"<span class=\"ts\"/><span class=\"ts\"/>return 1;", 
"<span class=\"ts\"/>}", 
"", 
"<span class=\"ts\"/>if (!(<a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#refresh\">refresh</a> == 50 || <a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#refresh\">refresh</a> == 60 || <a class=\"id\" href=\"#cvt\">cvt</a>.<a class=\"id\" href=\"#refresh\">refresh</a> == 70 ||", 
