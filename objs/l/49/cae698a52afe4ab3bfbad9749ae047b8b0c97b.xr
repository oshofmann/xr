"#<a class=\"id\" href=\"#define\">define</a> <a class=\"id\" href=\"#CGC_DATA_UNKNOWN\">CGC_DATA_UNKNOWN</a><span class=\"ts\"/>0", 
"#<a class=\"id\" href=\"#define\">define</a> <a class=\"id\" href=\"#CGC_DATA_WRITE\">CGC_DATA_WRITE</a><span class=\"ts\"/><span class=\"ts\"/>1", 
"#<a class=\"id\" href=\"#define\">define</a> <a class=\"id\" href=\"#CGC_DATA_READ\">CGC_DATA_READ</a><span class=\"ts\"/><span class=\"ts\"/>2", 
"#<a class=\"id\" href=\"#define\">define</a> <a class=\"id\" href=\"#CGC_DATA_NONE\">CGC_DATA_NONE</a><span class=\"ts\"/><span class=\"ts\"/>3", 
