"static int <a class=\"id\" href=\"#acpi_thermal_trips_update\">acpi_thermal_trips_update</a>(struct <a class=\"id\" href=\"#acpi_thermal\">acpi_thermal</a> *<a class=\"id\" href=\"#tz\">tz</a>, int <a class=\"id\" href=\"#flag\">flag</a>)", 
"{", 
"<span class=\"ts\"/><a class=\"id\" href=\"#acpi_status\">acpi_status</a> <a class=\"id\" href=\"#status\">status</a> = <a class=\"id\" href=\"#AE_OK\">AE_OK</a>;", 
"<span class=\"ts\"/>unsigned long long <a class=\"id\" href=\"#tmp\">tmp</a>;", 
"<span class=\"ts\"/>struct <a class=\"id\" href=\"#acpi_handle_list\">acpi_handle_list</a> <a class=\"id\" href=\"#devices\">devices</a>;", 
"<span class=\"ts\"/>int <a class=\"id\" href=\"#valid\">valid</a> = 0;", 
"<span class=\"ts\"/>int <a class=\"id\" href=\"#i\">i</a>;", 
"", 
"<span class=\"ts\"/><span class=\"comment\">/* Critical Shutdown */</span>", 
"<span class=\"ts\"/>if (<a class=\"id\" href=\"#flag\">flag</a> &amp; <a class=\"id\" href=\"#ACPI_TRIPS_CRITICAL\">ACPI_TRIPS_CRITICAL</a>) {", 
"<span class=\"ts\"/><span class=\"ts\"/><a class=\"id\" href=\"#status\">status</a> = <a class=\"id\" href=\"#acpi_evaluate_integer\">acpi_evaluate_integer</a>(<a class=\"id\" href=\"#tz\">tz</a>-><a class=\"id\" href=\"#device\">device</a>-><a class=\"id\" href=\"#handle\">handle</a>,", 
"<span class=\"ts\"/><span class=\"ts\"/><span class=\"ts\"/><span class=\"ts\"/>\"_CRT\", <a class=\"id\" href=\"#NULL\">NULL</a>, &amp;<a class=\"id\" href=\"#tmp\">tmp</a>);", 
"<span class=\"ts\"/><span class=\"ts\"/><a class=\"id\" href=\"#tz\">tz</a>-><a class=\"id\" href=\"#trips\">trips</a>.<a class=\"id\" href=\"#critical\">critical</a>.<a class=\"id\" href=\"#temperature\">temperature</a> = <a class=\"id\" href=\"#tmp\">tmp</a>;", 
"<span class=\"ts\"/><span class=\"ts\"/><span class=\"comment\">/*</span>", 
"<span class=\"comment\"><span class=\"ts\"/><span class=\"ts\"/> * Treat freezing temperatures as invalid as well; some</span>", 
"<span class=\"comment\"><span class=\"ts\"/><span class=\"ts\"/> * BIOSes return really low values and cause reboots at startup.</span>", 
"<span class=\"comment\"><span class=\"ts\"/><span class=\"ts\"/> * Below zero (Celsius) values clearly aren't right for sure..</span>", 
"<span class=\"comment\"><span class=\"ts\"/><span class=\"ts\"/> * ... so lets discard those as invalid.</span>", 
"<span class=\"comment\"><span class=\"ts\"/><span class=\"ts\"/> */</span>", 
"<span class=\"ts\"/><span class=\"ts\"/>if (<a class=\"id\" href=\"#ACPI_FAILURE\">ACPI_FAILURE</a>(<a class=\"id\" href=\"#status\">status</a>)) {", 
"<span class=\"ts\"/><span class=\"ts\"/><span class=\"ts\"/><a class=\"id\" href=\"#tz\">tz</a>-><a class=\"id\" href=\"#trips\">trips</a>.<a class=\"id\" href=\"#critical\">critical</a>.<a class=\"id\" href=\"#flags\">flags</a>.<a class=\"id\" href=\"#valid\">valid</a> = 0;", 
"<span class=\"ts\"/><span class=\"ts\"/><span class=\"ts\"/><a class=\"id\" href=\"#ACPI_DEBUG_PRINT\">ACPI_DEBUG_PRINT</a>((<a class=\"id\" href=\"#ACPI_DB_INFO\">ACPI_DB_INFO</a>,", 
"<span class=\"ts\"/><span class=\"ts\"/><span class=\"ts\"/><span class=\"ts\"/><span class=\"ts\"/>  \"No critical threshold\\n\"));", 
"<span class=\"ts\"/><span class=\"ts\"/>} else if (<a class=\"id\" href=\"#tmp\">tmp</a> <= 2732) {", 
"<span class=\"ts\"/><span class=\"ts\"/><span class=\"ts\"/><a class=\"id\" href=\"#printk\">printk</a>(<a class=\"id\" href=\"#KERN_WARNING\">KERN_WARNING</a> <a class=\"id\" href=\"#FW_BUG\">FW_BUG</a> \"Invalid critical threshold \"", 
"<span class=\"ts\"/><span class=\"ts\"/><span class=\"ts\"/>       \"(%llu)\\n\", <a class=\"id\" href=\"#tmp\">tmp</a>);", 
